{
  "name": "baseballpvp",
  "version": "0.0.1",
  "description": "A baseball game!",
  "main": "index.js",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "test": "mocha",
    "build": "cd client && yarn build",
    "make": "yarn install && cd client && yarn install"
  },
  "keywords": [
    "baseball"
  ],
  "author": "DavidLee",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "npm": "^6.1.0",
    "path": "^0.12.7",
    "sequelize": "^4.37.10",
    "sqlite3": "^4.0.0"
  }
}
